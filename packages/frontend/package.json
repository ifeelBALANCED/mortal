{
  "name": "@mortal/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix \"./**/**/*.{js,jsx,ts,tsx}\"",
    "test": "exit 0",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "clean": "rimraf dist",
    "depcruise": "npx depcruise --config .dependency-cruiser.cjs src",
    "depcruise:archi": "npx depcruise --version && depcruise --config .dependency-cruiser.cjs --output-type archi src | dot -T svg | depcruise-wrap-stream-in-html > depcruise-graph.html"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "cssnano": "^5.1.15",
    "framer-motion": "^10.8.5",
    "immer": "^9.0.21",
    "lerna": "^6.4.1",
    "nx": "^15.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/cz-commitlint": "^17.4.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.15.9",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.49.0",
    "dependency-cruiser": "^12.7.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.0",
    "lint-staged": "^13.1.0",
    "pkg-dir": "^7.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "rimraf": "^4.1.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
